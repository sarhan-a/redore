<div class="container">
  <h1>Available dates for <%= link_to @listing.title, listing_path(@listing) %></h1>
  <% @listing.collection_dates.each do |collection_date| %>
    <p>Date: <%= collection_date.date.to_date.strftime('%B %e, %Y') %> <%= link_to 'View Date', listing_collection_date_path(@listing, collection_date) %></p>
  <% end %>
</div>
